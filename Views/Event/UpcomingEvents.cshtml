@model List<EventViewModel>
@{

    ViewData["Title"] = "Upcomaing Events";

}
<h1>Upcoming Events</h1>
<table class="table table-striped table-bordered">

    <thead>

        <tr>

            <th>Id</th>

            <th>Title</th>

            <th>Description</th>

            <th>Date</th>

            <th>Address</th>

            <th>AssignedUser</th>

        </tr>

    </thead>

    <tbody>

        @foreach (EventViewModel eventItem in Model)

        {

            <tr>

                <td>@eventItem.Id</td>

                <td>@eventItem.Name</td>

                <td>@eventItem.Description</td>

                <td>@eventItem.Date.ToString("dd-MM-yyyy")</td>

                <td>@eventItem.Address</td>

                <td>@eventItem.AssignedUser</td>

            </tr>

        }

    </tbody>

</table>
