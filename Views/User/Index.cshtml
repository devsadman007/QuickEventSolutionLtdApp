@model List<UserViewModel>
@{
    ViewData["Title"] = "User List";
}
<h1>User List</h1>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Contact</th>
            <th>Email</th>
            <th>User Type</th>
        </tr>
    </thead>
    <tbody>
        @foreach (UserViewModel user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.ContactNumber</td>
                <td>@user.Email</td>
                <td>@user.UserType.ToString()</td>
            </tr>
        }
    </tbody>
</table>